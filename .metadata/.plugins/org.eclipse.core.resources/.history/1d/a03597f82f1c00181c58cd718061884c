package Exam1;

class Car {
	final int year;
	
	public Car() {year = 10;
		year = 1;}
	
	public void initiate() {
		System.out.println("Wenggggg");
	}
	
}

class ElectricCar extends Car {
	public ElectricCar() {}
	
	@Override
	public void initiate() {
		System.out.println("Wiiii");
	}
	
	public void warn() {
		System.out.println("Low battery");
	}
	
	
}


public class Track{
	public static void main(String[] args) {
		Car eCar = new ElectricCar();
		Car aCar = new Car();
		
		((ElectricCar) aCar).initiate();
		System.out.println(e instanceof ElectricCar);
		
	}
}